{
    "name": "genealabs/nova-gutenberg",
    "description": "Implementation of the Gutenberg editor as a Laravel Nova Field based on Laraberg.",
    "keywords": [
        "laraberg",
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "laravel/nova": "^3.0",
        "symfony/thanks": "^1.2",
        "unisharp/laravel-filemanager": "^2.0",
        "van-ons/laraberg": "^1.1"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.0",
        "phpmd/phpmd": "^2.8",
        "phpstan/phpstan": "^0.12.33",
        "phpunit/phpunit": "^8.5",
        "sebastian/phpcpd": "^5.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\NovaGutenberg\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\NovaGutenberg\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
